<div class="day-details previous-day">
  <p class="date"><b>Date: </b> <%= data.date %></p>

  <div class="tasks">
    <div class="form-group">
      <input type="checkbox" id="sql-<%= index %>" />
      <label><b>SQL</b></label>
    </div>

    <div class="form-group">
      <input type="checkbox" id="javascript-<%= index %>" />
      <label><b>JavaScript</b></label>
    </div>

    <div class="form-group">
      <input type="checkbox" id="reactjs-<%= index %>" />
      <label><b>ReactJS</b></label>
    </div>

    <div class="form-group">
      <input type="checkbox" id="dsa-<%= index %>" />
      <label><b>DSA</b></label>
    </div>

    <div class="form-group">
      <input type="checkbox" id="hackerrank-<%= index %>" />
      <label><b>HackerRank</b></label>
    </div>

    <div class="form-group">
      <input type="checkbox" id="project-<%= index %>" />
      <label><b>Project</b></label>
    </div>
  </div>
</div>

<script>
  const data = {
    sql: "<%= data.sql %>",
    javaScript: "<%= data.javaScript %>",
    reactJs: "<%= data.reactJs %>",
    dsa: "<%= data.dsa %>",
    hackerRank: "<%= data.hackerRank %>",
    project: "<%= data.project %>",
  };

  for (item in data) {
    const inputElement = document.getElementById(`${item.toLowerCase()}-${"<%= index %>"}`);

    if (data[item] === "true") {
      inputElement.checked = true;
    }
  }
</script>
